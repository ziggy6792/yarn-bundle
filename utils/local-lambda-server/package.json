{
  "name": "@yarn-monorepo/local-lambda-server",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "run-p start:mongo:db start:mongo:ui start:local ",
    "start:other": "run-p start:mongo:db start:mongo:ui",
    "force:stop": "echo forcestop $ kill $(lsof -i:3100 | grep node | cut -d' ' -f2- )",
    "start:local": "sleep 10 && ts-node-dev -r tsconfig-paths/register src/index.ts --watch .env --respawn src/index.ts ",
    "start:mongo:db": "docker-compose up > /dev/null",
    "start:mongo:ui": "mongo-gui -u mongodb://localhost:27016/"
  },
  "dependencies": {
    "@yarn-monorepo/lambda-gq-resolver": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lambda-local": "^1.7.4",
    "lodash": "^4.17.21",
    "mongo-gui": "^0.0.13",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.3.1"
  }
}
